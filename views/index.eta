<h2>Previous chats</h2>
  <ul>
  <% it.forEach((message) => { %>
    <li>
      <%= message.sender %>: <%= message.message %>
    </li>
  <% }); %>
  </ul>
<h2>Add a message</h2>

<form method="POST">
  Sender: <input type="text" name="sender" /><br/>
  Message: <input type="text" name="message" /><br/>
  <input type="submit" value="Submit!" />
</form>


